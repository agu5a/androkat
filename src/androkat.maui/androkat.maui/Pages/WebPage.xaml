<?xml version="1.0" encoding="utf-8" ?>
<ContentPage 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="WebOldalak"
    x:Name="this"
    
    xmlns:viewmodels="clr-namespace:androkat.hu.ViewModels"
    Style="{StaticResource MainSectionStyle}"
    
    x:Class="androkat.hu.Pages.WebPage">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical" Padding="10" Background="{AppThemeBinding Light={StaticResource Grey1}, Dark={StaticResource Grey9}}">
            <Picker x:Name="webPicker" Title="WebOldalak" SelectedIndexChanged="webPicker_SelectedIndexChanged" Style="{StaticResource BodySLabelStyle}">
        </Picker>
    </StackLayout>
    </ContentPage.Content>
</ContentPage>